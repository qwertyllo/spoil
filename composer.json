{
    "name": "qwertyllo/spoil",
    "type": "library",
    "description": "SPOIL is a PHP library for SharePoint Online (2013) and SharePoint for Office 365. Forked from impensavel/spoil",
    "keywords": [
        "sharepoint",
        "online",
        "2013",
        "office",
        "365",
        "oauth",
        "rest",
        "api",
        "client"
    ],
    "homepage": "https://github.com/qwertyllo/spoil",
    "license": "MIT",
    "authors": [
        {
            "name": "Qwertyllo Dev",
            "email": "qwertyllodev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "php-http/message": "^1.0",
        "firebase/php-jwt": "^3.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client": "^0.3.0",
        "phpunit/phpunit": "^4.0",
        "friendsofphp/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Impensavel\\Spoil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Impensavel\\Spoil\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "php-http/guzzle6-adapter": "Guzzle 6 HTTP adapter",
        "php-http/curl-client": "cURL client HTTP adapter"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix src --level=psr2 --verbose",
            "php-cs-fixer fix tests --level=psr2 --verbose"
        ],
        "test": "phpunit --verbose"
    }
}
